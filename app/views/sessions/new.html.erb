<h3> Enter your Username and Password to log in </h3>

<%= form_tag do %>

<%= label_tag :username %>
<%= text_field_tag :username %>

<%= label_tag :password %>
<%= password_field_tag :password %>

<%= submit_tag "Sign In" %>

<% end %>

<h4> Don't have an account? <%= link_to "Sign up here", new_user_path %> </h4>
